{
  "name": "figma-vue3-vite-starter",
  "description": "CODE.FUN Figma Plugin Starter",
  "author": "https://code.fun",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "vue-tsc --noEmit && vite build && vite build --config vite.config.figma.ts",
