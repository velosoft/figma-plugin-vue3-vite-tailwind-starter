    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.12",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.17",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-singlefile": "^0.12.1",
    "vue-tsc": "^0.40.4"
  }
}